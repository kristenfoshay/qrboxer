{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "location": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "date": {
        "type": "string"
      },
      "username": {
        "type": "string",
        "minLength": 1,
        "maxLength": 25
      }
    },
    "additionalProperties": false,
    "required": [
      "location",
      "date"
    ]
  }