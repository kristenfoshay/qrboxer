{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "from_address": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "to_address": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "date": {
        "type": "string",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      }
    },
    "additionalProperties": false,
    "minProperties": 1
}
